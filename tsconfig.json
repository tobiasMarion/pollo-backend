{
  "compilerOptions": {
    "module": "preserve",
    "moduleResolution": "bundler",
    "target": "ES2022",
    "allowImportingTsExtensions": true,
    "strictFunctionTypes": true,
    "strict": true,
    "noImplicitAny": true,
    "noEmit": true,
    "paths": {
      "@/*": [
        "./src/*"
      ]
    },
  }
}